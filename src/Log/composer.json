{
    "name": "moderntribe/square1-log",
    "type": "library",
    "description": "Provides a WordPress hook based PSR logger for square one",
    "license": "GPL-2.0-only",
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist"
    },
    "require": {
        "php": ">=7.4",
        "psr/log": "^1.1 || ^2.0",
        "moderntribe/square1-container": "^4.2.1",
        "mhcg/monolog-wp-cli": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Tribe\\Libs\\Log\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
