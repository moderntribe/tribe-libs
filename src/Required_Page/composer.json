{
    "name": "moderntribe/square1-required-page",
    "type": "library",
    "description": "A utility for forcing content pages to exist in square one",
    "license": "GPL-2.0-only",
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "require": {
        "moderntribe/square1-container": "^4.2.1",
